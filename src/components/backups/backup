      const history = useHistory();
      const scrollTarget = (target) => scroller.scrollTo(target, {smooth: true, duration: 700}); 
  
      const scrollToPage = async(target)=>{
          if (history.location.pathname !=='/') {
              await history.push('/');
          }
          scrollTarget(target);
      }
  